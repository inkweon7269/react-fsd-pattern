{
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "tabWidth": 2,
    "bracketSpacing": true,
    "endOfLine": "auto",
    "useTabs": false,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 200
        }
      }
    ],
    "importOrder": ["<THIRD_PARTY_MODULES>", "^@/", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
  